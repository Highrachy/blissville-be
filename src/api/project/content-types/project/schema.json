{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "default": "Generated automatically based on the name"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "type": "string"
    },
    "street1": {
      "type": "text",
      "required": true
    },
    "street2": {
      "type": "text"
    },
    "city": {
      "type": "text",
      "required": true
    },
    "state": {
      "type": "text",
      "required": true
    },
    "features": {
      "type": "text",
      "required": true
    },
    "standardFeatures": {
      "type": "text"
    },
    "supremeFeatures": {
      "type": "text"
    },
    "paymentPlan": {
      "type": "integer",
      "default": 0
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "delivery": {
      "type": "date",
      "required": true
    },
    "status": {
      "type": "integer",
      "default": 0
    },
    "properties": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::property.property",
      "mappedBy": "project"
    },
    "neighborhoods": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::neighborhood.neighborhood",
      "mappedBy": "project"
    },
    "faqs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faq.faq",
      "mappedBy": "project"
    },
    "project_galleries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-gallery.project-gallery",
      "mappedBy": "project"
    }
  }
}
