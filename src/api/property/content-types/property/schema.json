{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "default": "Generated automatically based on the name"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "type": "string"
    },
    "size": {
      "type": "integer",
      "default": 0
    },
    "totalUnits": {
      "type": "integer",
      "default": 0
    },
    "availableUnits": {
      "type": "integer",
      "default": 0
    },
    "baths": {
      "type": "integer",
      "default": 0
    },
    "beds": {
      "type": "integer",
      "default": 0
    },
    "toilets": {
      "type": "integer",
      "default": 0
    },
    "floors": {
      "type": "string",
      "default": "Ground floor"
    },
    "parkingSpace": {
      "type": "integer",
      "default": 0
    },
    "price": {
      "type": "biginteger",
      "default": 0
    },
    "standardPrice": {
      "type": "biginteger",
      "default": 0
    },
    "supremePrice": {
      "type": "biginteger",
      "default": 0
    },
    "status": {
      "type": "integer",
      "default": 0
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "properties"
    },
    "initialPayment": {
      "type": "biginteger"
    },
    "paymentPlan": {
      "type": "integer",
      "default": 0
    },
    "property_galleries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::property-gallery.property-gallery",
      "mappedBy": "property"
    },
    "floor_plans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::floor-plan.floor-plan",
      "mappedBy": "property"
    }
  }
}
